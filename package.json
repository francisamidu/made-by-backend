{
  "name": "made-by-backend",
  "version": "0.0.0",
  "description": "Made By backend built with Typescript, Express, Postgresql and Drizzle",
  "type": "module",
  "engines": {
    "node": ">= 22.11 < 23"
  },
  "devDependencies": {
    "@eslint/js": "~9.17",
    "@types/bcrypt": "~10.0.28-alpha",
    "@types/cors": "~2.8.17",
    "@types/eslint__js": "~8.42",
    "@types/express": "~5.0.0",
    "@types/express-session": "~1.18.1",
    "@types/jsonwebtoken": "~9.0.9",
    "@types/multer": "^1.4.12",
    "@types/node": "~20",
    "@types/passport": "~1.0.17",
    "@types/passport-github2": "~1.2.9",
    "@types/passport-google-oauth20": "~2.0.16",
    "@types/passport-linkedin-oauth2": "~1.5.6",
    "@types/passport-local": "^1.0.38",
    "@types/passport-twitter": "~1.0.40",
    "@types/pg": "^8.11.11",
    "@types/qs": "~6.9.18",
    "@types/swagger-jsdoc": "~6.0.4",
    "@types/swagger-ui-express": "~4.1.7",
    "@types/zxcvbn": "~4.4.5",
    "@typescript-eslint/parser": "~8.19",
    "prettier": "~3.4",
    "rimraf": "~6.0",
    "ts-api-utils": "~2.0",
    "typescript": "~5.7",
    "vitest": "~2.1"
  },
  "scripts": {
    "db:ui": "bun drizzle-kit studio --port 3000 --verbose",
    "db:generate": "bun drizzle-kit generate",
    "db:migrate": "bun drizzle-kit migrate",
    "db:help": "bun drizzle-kit push -h",
    "db:up": "bun drizzle-kit up",
    "db:push": "bun drizzle-kit push",
    "db:seed": "bun src/db/seeder.ts",
    "start": "node dist/app.js",
    "dev": "bun --watch src/app.ts",
    "build": "tsc && tsc-alias",
    "lint": "eslint .",
    "prettier": "prettier \"{src,__{tests}__}/**/*.{ts,mts}\" --config .prettierrc --write",
    "prettier:check": "prettier \"{src,__{tests}__}/**/*.{ts,mts}\" --config .prettierrc --check"
  },
  "author": "Francis Amidu",
  "license": "MIT",
  "dependencies": {
    "@types/express-serve-static-core": "^5.0.6",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^3.0.2",
    "cloudinary": "^2.5.1",
    "console-log-json": "^3.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "dotenv-expand": "^12.0.1",
    "drizzle-kit": "^0.30.5",
    "drizzle-orm": "^0.39.1",
    "express": "^4.21.2",
    "express-list-endpoints": "^7.1.1",
    "express-session": "^1.18.1",
    "express-validator": "^7.2.1",
    "helmet": "^8.0.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "passport": "^0.7.0",
    "passport-github2": "^0.1.12",
    "passport-google-oauth20": "^2.0.0",
    "passport-local": "^1.0.0",
    "passport-twitter": "^1.0.4",
    "pg": "^8.13.1",
    "pino": "^9.6.0",
    "pino-http": "^10.4.0",
    "pino-pretty": "^13.0.0",
    "qs": "^6.14.0",
    "sharp": "^0.33.5",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "tsc-alias": "^1.8.10",
    "tsc-watch": "^6.2.1",
    "tsconfig-paths": "^4.2.0",
    "tslib": "~2.8",
    "uuid": "^11.0.5",
    "zod": "^3.24.1",
    "zxcvbn": "^4.4.2"
  },
  "volta": {
    "node": "22.12.0"
  }
